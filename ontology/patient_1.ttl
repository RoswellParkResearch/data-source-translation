# axioms for prefixes'
@base <http://purl.obolibrary.org/obo/db_mapping.owl/> .
@prefix : <http://purl.obolibrary.org/obo/db_mapping.owl/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# custom prefixes
@prefix table: <table/patients_1/> .
@prefix filename: <table/patients_1> .
@prefix field: <field/patients_1/> .
@prefix field_value: <field_value/patients_1/> .
@prefix fv: <field_value/patients_1/> .
@prefix record: <record/patients_1/> .
@prefix data_property: <data_property/patients_1/> .

# imports db mapping ontology
[rdf:type owl:Ontology ;
   owl:imports <http://purl.obolibrary.org/obo/db_mapping.owl> ] .

# axioms to create table
filename: rdf:type owl:Class; rdfs:subClassOf :table .
filename:_i rdf:type filename: .
field:patient_id rdf:type owl:Class; rdfs:subClassOf :field .
field:patient_id_i rdf:type field:patient_id .
field:patient_id_i :member_of filename:_i .
data_property:patient_id_data_value rdf:type owl:DatatypeProperty .
field:gender rdf:type owl:Class; rdfs:subClassOf :field .
field:gender_i rdf:type field:gender .
field:gender_i :member_of filename:_i .
data_property:gender_data_value rdf:type owl:DatatypeProperty .
field:birth_date rdf:type owl:Class; rdfs:subClassOf :field .
field:birth_date_i rdf:type field:birth_date .
field:birth_date_i :member_of filename:_i .
data_property:birth_date_data_value rdf:type owl:DatatypeProperty .

# axioms to create fields
field:patient_id rdf:type owl:Class; rdfs:subClassOf :field .
field:patient_id_i rdf:type field:patient_id .
field:patient_id_i :member_of filename:_i .
data_property:patient_id_data_value rdf:type owl:DatatypeProperty .
field:gender rdf:type owl:Class; rdfs:subClassOf :field .
field:gender_i rdf:type field:gender .
field:gender_i :member_of filename:_i .
data_property:gender_data_value rdf:type owl:DatatypeProperty .
field:birth_date rdf:type owl:Class; rdfs:subClassOf :field .
field:birth_date_i rdf:type field:birth_date .
field:birth_date_i :member_of filename:_i .
data_property:birth_date_data_value rdf:type owl:DatatypeProperty .


# axioms to create records
record:patients_1_record_1 rdf:type :record .
record:patients_1_record_1 :member_of filename:_i .
record:patients_1_record_2 rdf:type :record .
record:patients_1_record_2 :member_of filename:_i .
record:patients_1_record_3 rdf:type :record .
record:patients_1_record_3 :member_of filename:_i .
record:patients_1_record_4 rdf:type :record .
record:patients_1_record_4 :member_of filename:_i .
record:patients_1_record_5 rdf:type :record .
record:patients_1_record_5 :member_of filename:_i .
record:patients_1_record_6 rdf:type :record .
record:patients_1_record_6 :member_of filename:_i .
record:patients_1_record_7 rdf:type :record .
record:patients_1_record_7 :member_of filename:_i .
record:patients_1_record_8 rdf:type :record .
record:patients_1_record_8 :member_of filename:_i .
record:patients_1_record_9 rdf:type :record .
record:patients_1_record_9 :member_of filename:_i .
record:patients_1_record_10 rdf:type :record .
record:patients_1_record_10 :member_of filename:_i .
