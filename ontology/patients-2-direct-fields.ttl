# axioms for prefixes
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# custom prefixes
@base <http://purl.obolibrary.org/obo/data-source/patients_2/> .
@prefix : <http://purl.obolibrary.org/obo/data-source/patients_2/> .
@prefix ds: <http://purl.obolibrary.org/obo/data-source-ontology.owl/> .
@prefix op: <object_property/> .
@prefix dp: <data_property/> .
@prefix df: <data_field/> .
@prefix dr: <data_record/> .

@prefix data_field: <http://purl.obolibrary.org/obo/data-source-ontology.owl/data_field> .
@prefix data_record: <http://purl.obolibrary.org/obo/data-source-ontology.owl/data_record> .
@prefix has_value: <http://purl.obolibrary.org/obo/data-source-ontology.owl/has_value> .
@prefix has_member: <http://purl.obolibrary.org/obo/data-source-ontology.owl/has_member> .
@prefix has_data_field: <http://purl.obolibrary.org/obo/data-source-ontology.owl/has_data_field> .
@prefix has_data_record: <http://purl.obolibrary.org/obo/data-source-ontology.owl/has_data_record> .
@prefix member_of: <http://purl.obolibrary.org/obo/data-source-ontology.owl/member_of> .
@prefix data_field_of: <http://purl.obolibrary.org/obo/data-source-ontology.owl/data_field_of> .
@prefix data_record_of: <http://purl.obolibrary.org/obo/data-source-ontology.owl/data_record_of> .

# ontology uri
<http://purl.obolibrary.org/obo/data-source/patients_2/patients_2.owl> rdf:type owl:Ontology .

# specified ontology imports
<http://purl.obolibrary.org/obo/data-source/patients_2/patients_2.owl> owl:imports <http://purl.obolibrary.org/obo/data-source-ontology.owl> . 

## specify data source
<http://purl.obolibrary.org/obo/data-source/patients_2/spreadsheet/patients_2> rdf:type owl:NamedIndividual, ds:spreadsheet .


# axioms to create field specific object properties

# axioms to create object properties
op:has_subject_id rdf:type owl:ObjectProperty .
op:has_subject_id rdfs:subPropertyOf ds:has_data_field .
op:subject_id_of rdf:type owl:ObjectProperty .
op:subject_id_of rdfs:subPropertyOf ds:data_field_of .
op:has_sex rdf:type owl:ObjectProperty .
op:has_sex rdfs:subPropertyOf ds:has_data_field .
op:sex_of rdf:type owl:ObjectProperty .
op:sex_of rdfs:subPropertyOf ds:data_field_of .
op:has_DOB rdf:type owl:ObjectProperty .
op:has_DOB rdfs:subPropertyOf ds:has_data_field .
op:DOB_of rdf:type owl:ObjectProperty .
op:DOB_of rdfs:subPropertyOf ds:data_field_of .

# axioms to create data field classes

# axioms to data field classes
df:subject_id_field rdf:type owl:Class .
df:subject_id_field rdfs:subClassOf ds:data_field .
df:sex_field rdf:type owl:Class .
df:sex_field rdfs:subClassOf ds:data_field .
df:DOB_field rdf:type owl:Class .
df:DOB_field rdfs:subClassOf ds:data_field .

# axioms to create data record class
dr:patients_2_record rdf:type owl:Class .
dr:patients_2_record rdfs:subClassOf ds:data_record .
# axioms to create data records and fields
# axioms to create dr:data_record_1_i
dr:data_record_1_i 
     rdf:type owl:NamedIndividual, dr:patients_2_record; 
     ds:data_record_of <spreadsheet/patients_2.xlsx> .

# axioms to create data fields for dr:data_record_1_i
df:subject_id_field_1_i 
   rdf:type owl:NamedIndividual, df:subject_id_field; 
   ds:has_value "P1";
   op:subject_id_of dr:data_record_1_i .
dr:data_record_1_i 
   op:has_subject_id df:subject_id_field_1_i .  

df:sex_field_1_i 
   rdf:type owl:NamedIndividual, df:sex_field; 
   ds:has_value "Male";
   op:sex_of dr:data_record_1_i .
dr:data_record_1_i 
   op:has_sex df:sex_field_1_i .  

df:DOB_field_1_i 
   rdf:type owl:NamedIndividual, df:DOB_field; 
   ds:has_value "1953-01-01";
   op:DOB_of dr:data_record_1_i .
dr:data_record_1_i 
   op:has_DOB df:DOB_field_1_i .  

